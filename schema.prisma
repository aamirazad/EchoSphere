datasource db {
  // could be postgresql or mysql
  provider = "sqlite"
  url      = "file:main.db"
}

generator db {
  provider             = "prisma-client-py"
  interface            = "asyncio"
  recursive_type_depth = 5
}

model Tweet {
  id         String   @id @default(cuid())
  created_at DateTime @default(now())
  title      String
  username   String
  published  Boolean
}

model Users {
  id         String   @id @default(cuid())
  username   String
  avatar     String
}